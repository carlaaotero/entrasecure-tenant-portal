<div class="confirm-modal" data-confirm-modal>
  <div class="confirm-modal-card">
    <h3 data-confirm-title><%= title %></h3>
    <p data-confirm-message><%= message %></p>
    <div class="confirm-modal-actions">
      <button class="btn btn-outline" data-confirm-cancel><%= cancelText %></button>
      <button class="btn btn-primary" data-confirm-ok><%= okText %></button>
    </div>
  </div>
</div>
